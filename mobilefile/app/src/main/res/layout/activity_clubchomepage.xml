<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".clubchomepage"
    android:background="@color/purple_200">

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="63dp"
        android:layout_marginTop="62dp"
        android:text="Welcome to \nClub C"
        android:textSize="38dp"
        android:textAlignment="center"/>

    <Button
        android:id="@+id/button_sessionc"
        android:layout_width="280dp"
        android:layout_height="110dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="40dp"
        android:layout_marginTop="175dp"
        android:text="Sessions"
        android:textSize="25dp"
        android:backgroundTint="#ABF2FB"
        android:textColor="@color/black"/>

    <Button
        android:id="@+id/button_memberc"
        android:layout_width="280dp"
        android:layout_height="110dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerInParent="true"
        android:layout_marginStart="40dp"
        android:layout_marginTop="286dp"
        android:text="Members"
        android:textSize="25dp"
        android:backgroundTint="#ABF2FB"
        android:textColor="@color/black"/>

    <Button
        android:id="@+id/button_scoreboardc"
        android:layout_width="280dp"
        android:layout_height="110dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="40dp"
        android:layout_marginBottom="158dp"
        android:text="Scoreboard"
        android:textSize="25dp"
        android:backgroundTint="#ABF2FB"
        android:textColor="@color/black"/>

    <Button
        android:id="@+id/button_leaderboardc"
        android:layout_width="280dp"
        android:layout_height="110dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="40dp"
        android:layout_marginBottom="48dp"
        android:text="Leaderboard"
        android:textSize="25dp"
        android:backgroundTint="#ABF2FB"
        android:textColor="@color/black"/>

</RelativeLayout>